        </activity>
        <activity android:name="lab1.fedorenko.nure.ua.ua.nure.fedorenko.lab1.CalculatorActivity" />
        <activity android:name="lab1.fedorenko.nure.ua.ua.nure.fedorenko.lab1.ColorEditorActivity" />

